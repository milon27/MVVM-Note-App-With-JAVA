<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.m27lab.m27labnotemvvm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch_note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launch_note_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activity.SingleNoteActivity"></activity>
        <activity android:name=".views.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>